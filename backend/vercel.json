
  
  {
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "headers": [
      { "source": "/api/user/signup", "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }] }
    ],
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ]
  }
  